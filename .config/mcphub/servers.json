{
  "nativeMCPServers": [],
  "mcpServers": {
    "time": {
      "command": "docker",
      "autoApprove": [
        "get_current_time",
        "convert_time"
      ],
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/time"
      ]
    },
    "terminal-controller": {
      "command": "uvx",
      "args": [
        "terminal_controller"
      ]
    }
  }
}
